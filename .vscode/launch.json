{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Nest Debugger",
      "args": [
          "${workspaceFolder}/src/main.ts"
      ],
      "runtimeArgs": [
          "--nolazy",
          "-r",
          "ts-node/register",
          "-r",
          "tsconfig-paths/register"
      ],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}/src",
      "protocol": "inspector"
    }
  ]
}